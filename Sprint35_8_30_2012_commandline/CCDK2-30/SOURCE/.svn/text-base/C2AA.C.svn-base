#include <bbsdefs.h>
#ifdef   SPLINE
#include <c2adefs.h>
#include <c2apriv.h>

/*-------------------------------------------------------------------------*/
BBS_PRIVATE BOOLEAN c2a_non_rational ( a, n ) 
/*-------------------------------------------------------------------------*/
/*!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!*/
/*!!!!!!!!                                                          !!!!!!!*/
/*!!!!!!!!      (C) Copyright 1989 - 1995 Building Block Software   !!!!!!!*/
/*!!!!!!!!                    All rights reserved                   !!!!!!!*/
/*!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!*/
HPT2 *a ;
INT n ;
{

    INT i ;

    for ( i=0 ; i<n ; i++ ) 
        if ( !IS_ZERO ( a[i][2]-1.0 ) ) 
            RETURN ( FALSE ) ;
    RETURN ( TRUE ) ;
}

#endif   /*SPLINE*/


